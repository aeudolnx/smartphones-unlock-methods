// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Smartphone {
  imagemDoSmartphone Bytes
  model              String @id @unique
  nomeDoCelular      String
  processador        String
  binarios           String
  androids           String
  downloadMode       String
  factoryReset       String
  unlockMethod       String
  allowBinary        String
  notAllowBinary     String
  flashRom           String
  program            String
}
